ue === null) {\r\n            return; //break out of the function early\r\n        }\r\n        const newtodos = this.state.todos.map(todo => {\r\n            if (todo.id === id) {\r\n                todo.text = value.replace(/\\s+/g, ' ').trim();\r\n            }\r\n            return todo;\r\n        });\r\n        this.setState({ todos: newtodos });\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div className=\"container\">\r\n                <Card style={{ width: '20rem', boxShadow: \"0 4px 8px 0 rgba(0,0,0,0.2)\" }}>\r\n                    <ToDoForm onSubmit={this.addtoList} />\r\n                    <ul class=\"list-group\">\r\n                        <ToDo todolist={this.state.todos} onChildDeleteClick={this.clickDelete} onChildEditClick={this.clickEdit} />\r\n                    </ul>\r\n                </Card>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default ToDoList\r\n","D:\\Office\\checklistapplication\\src\\components\\todo.js",[],{"ruleId":"22","severity":1,"message":"23","line":3,"column":11,"nodeType":"24","messageId":"25","endLine":3,"endColumn":30},{"ruleId":"22","severity":1,"message":"26","line":3,"column":31,"nodeType":"24","messageId":"25","endLine":3,"endColumn":43},{"ruleId":"22","severity":1,"message":"27","line":4,"column":16,"nodeType":"24","messageId":"25","endLine":4,"endColumn":22},"no-unused-vars","'WhatsappShareButton' is defined but never used.","Identifier","unusedVar","'WhatsappIcon' is defined but never used.","'Button' is defined but never used."]